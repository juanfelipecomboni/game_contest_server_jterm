<div class='col-md-offset-3 col-md-6'>
    <h1>Update Tournament </h1>

    <%= form_for @tournament do |f| %>
        <%= render 'shared/error_messages', object: @tournament %>
        <%= render 'fields', f: f %>

        <%= f.submit "Update Tournament", class: 'btn btn-primary' %>
    <% end %>
</div>
